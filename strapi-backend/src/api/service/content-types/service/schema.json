{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "duration_minutes": {
      "type": "integer",
      "required": true
    },
    "is_active": {
      "type": "boolean",
      "default": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service-category.service-category",
      "inversedBy": "services"
    },
    "booking_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking-item.booking-item",
      "mappedBy": "service"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "service"
    }
  }
}