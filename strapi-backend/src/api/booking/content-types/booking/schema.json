{
  "kind": "collectionType",
  "collectionName": "bookings",
  "info": {
    "singularName": "booking",
    "pluralName": "bookings",
    "displayName": "Booking",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "booking_date": {
      "type": "date",
      "required": true
    },
    "start_time": {
      "type": "time",
      "required": true
    },
    "end_time": {
      "type": "time",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "confirmed",
        "completed",
        "cancelled"
      ],
      "default": "pending",
      "required": true
    },
    "total_price": {
      "type": "decimal",
      "required": true
    },
    "notes": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::user.user",
      "inversedBy": "bookings"
    },
    "vehicle": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vehicle.vehicle",
      "inversedBy": "bookings"
    },
    "booking_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking-item.booking-item",
      "mappedBy": "booking"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "booking"
    }
  }
}