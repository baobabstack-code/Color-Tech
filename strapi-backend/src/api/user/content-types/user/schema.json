{
  "kind": "collectionType",
  "collectionName": "users",
  "info": {
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "pluginOptions": {
    "users-permissions": {
      "delete": false
    }
  },
  "attributes": {
    "first_name": {
      "type": "string",
      "required": true
    },
    "last_name": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "text"
    },
    "role": {
      "type": "enumeration",
      "enum": [
        "client",
        "staff",
        "admin"
      ],
      "default": "client",
      "required": true
    },
    "is_active": {
      "type": "boolean",
      "default": true
    },
    "vehicles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vehicle.vehicle",
      "mappedBy": "user"
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "user"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "user"
    },
    "contents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::content.content",
      "mappedBy": "author"
    }
  }
}